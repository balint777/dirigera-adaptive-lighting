{
  "env": {
    "es2021": true
  },
  "extends": ["standard", "plugin:jsdoc/recommended-error"],
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [
        ".eslintrc.{js,cjs}"
      ],
      "parserOptions": {
        "sourceType": "module"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
  },
  "plugins": [
    "jsdoc"
  ]
}
